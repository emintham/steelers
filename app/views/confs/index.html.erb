<h1>Listing confs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Program</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @confs.each do |conf| %>
  <tr>
    <td><%= conf.name %></td>
    <td><%= conf.program %></td>
    <td><%= link_to 'Show', conf %></td>
    <td><%= link_to 'Edit', edit_conf_path(conf) %></td>
    <td><%= link_to 'Destroy', conf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Conf', new_conf_path %>
