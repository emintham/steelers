<table class="table table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Program</th>
      <th>Created at</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @config_templates.each do |config_template| %>
      <tr>
        <td><%= config_template.name %></td>
        <td><%= Program.find(config_template.program_id).name %></td>
        <td><%= config_template.created_at.to_date %></td>
        <td><%= link_to 'Show', config_template,
          class: 'btn btn-mini' %></td>
        <td><%= link_to 'Edit', edit_config_template_path(config_template),
          class: 'btn btn-mini' %></td>
        <td><%= link_to 'Delete', config_template, 
          method: :delete, data: { confirm: 'Are you sure?' },
          class: 'btn btn-mini' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br />
<%= link_to 'New Template', new_config_template_path, 
  class: 'btn btn-mini' %>
