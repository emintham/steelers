<% content_for :title do %>Steelers- Home<% end %>
<% content_for :description do %>User home page<% end %>

<div class='page-header'>
  <h3>
    Logged in as: <%= link_to @user.user_id, user_path(@user) %>
  </h3>
  <p>Current time: <%= Time.now.asctime %></p>
  <%= link_to 'New Job', new_user_job_path(@user), 
    :class => 'btn btn-mini' %>
</div>

<div class="span10">
  <div class="accordion" id="accordion1">
    <%= render 'job_queue' %>
    <%= render 'configs' %>
    <%= render 'log_queue' %>
    <%= render 'upload_index' %>
  </div> <!-- accordion -->
</div>

<div class="span10">
  <hr>
</div>

<%= render 'new_upload' %>
